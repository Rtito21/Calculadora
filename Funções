float somarX (float num1, float num2)
{
    float result;
    result = num1 + num2;
    return result;
}

float subtrairX (float num1, float num2)
{
    float result;
    result = num1 - num2;
    return result;
}

float dividirX (float num1, float num2)
{
    float result;
    result = num1 / num2;
    return result;
}

float multiplicarX (float num1, float num2)
{
    float result;
    result = num1 * num2;
    return result;
}
